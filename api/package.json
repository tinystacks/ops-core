{
  "name": "ops-api",
  "version": "0.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "clean": "rm -rf ./dist",
    "clean-build": "npm run clean && npm ci && npm run build",
    "dev": "bash ./dev.sh",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src",
    "lint-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json ./test",
    "lint-fix-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json --fix ./test",
    "setup-test-lint": "node ./scripts/setup-test-lint.js",
    "start": "node ./dist/server.js",
    "test": "jest",
    "test-cov": "jest --coverage",
    "test-file": "jest ./test/clients/console-client/local.test.ts",
    "view-test-cov": "jest --coverage || true && open coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.isnil": "^4.0.7",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.0",
    "lodash.isempty": "^4.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@tinystacks/ops-model": "file:local-deps/tinystacks-ops-model-1.0.0.tgz",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi": "^12.1.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "lodash.isnil": "^4.0.0",
    "swagger-ui-express": "^4.6.0",
    "yamljs": "^0.3.0"
  }
}

{
  "name": "@tinystacks/ops-core",
  "version": "0.0.1-release",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinystacks/ops-core.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf *.tgz; rm -rf ./dist; rm -rf node_modules; rm package-lock.json",
    "clean-build": "npm run clean; npm i; npm run build",
    "prepack": "npm run clean-build",
    "dev": "bash ./dev.sh",
    "prerelease": "npm version prerelease --preid=local --no-git-tag-version",
    "publish-local": "npm run prerelease; npm publish --tag local --@tinystacks:registry=http://local-npm-registry:4873",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src",
    "lint-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json ./test",
    "lint-fix-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json --fix ./test",
    "setup-test-lint": "node ./scripts/setup-test-lint.js",
    "start": "node ./dist/server.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@types/express": "^4.17.16",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isnil": "^4.0.7",
    "@types/lodash.upperfirst": "^4.3.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-cli": "^6.26.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.0",
    "lodash.isempty": "^4.4.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@tinystacks/ops-model": "0.0.0-release",
    "@types/react": "^18.0.28",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi": "^12.1.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "lodash.get": "^4.4.2",
    "lodash.isnil": "^4.0.0",
    "swagger-ui-express": "^4.6.0",
    "yamljs": "^0.3.0"
  }
}
